{
  "name": "Peace",
  "description": "Peace - Code editor for Chrome and ChromeOS",
  "manifest_version": 2,  
  "version": "0.0.1",
  "minimum_chrome_version": "30",
  "icons": {
    "16": "img/icon_16.png",
    "128": "img/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["js/main.js"]
    }
  },
  "permissions": ["webview"]
}
